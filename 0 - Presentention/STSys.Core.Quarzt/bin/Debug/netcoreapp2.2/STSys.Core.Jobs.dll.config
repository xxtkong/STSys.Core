<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 数据库类型:MySql/SqlServer -->
    <add key="DbType" value="SqlServer" />
    <!-- 服务名 -->
    <add key="ServiceName" value="STSys.Core.Jobs" />
    <!-- 服务显示名 -->
    <add key="ServiceDisplayName" value="ST.Only.Jobs" />
    <!-- 表名映射-BackgroundJob表名映射   Value为数据库表名-->
    <add key="BackgroundJobMappingDbTable" value="BackgroundJob" />
    <!-- 表名映射-BackgroundJobLog表名映射   Value为数据库表名-->
    <add key="BackgroundJobLogMappingDbTable" value="BackgroundJobLog" />
    <!-- MongDB -->
    <add key="MongDB" value="Servers=192.168.0.109;ConnectTimeout=300000;ConnectionLifetime=300000;MinimumPoolSize=8;MaximumPoolSize=256;Pooled=true" />
    <add key="webDomain" value="http://localhost:80" />
    <add key="webdir" value="D:\website\Web" />
  </appSettings>
  <connectionStrings>
    <!-- 数据库连接字符串 根据DbType的配置类型进行调整-->
    <add name="connectionString" connectionString="Data Source=127.0.0.1;User ID=sa;Password=123456;Initial Catalog=Only.Jobs;Pooling=true;" providerName="System.Data.SqlClient" />
    <add name="strSqlCe" connectionString="Data Source=192.168.0.109;Initial Catalog=STSys;Persist Security Info=True;User ID=sa;Password=123456;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>